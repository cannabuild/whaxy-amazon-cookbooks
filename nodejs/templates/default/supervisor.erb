[program:<%= node['deploy'][@webapp_name][:environment][:dns_name] %>]
environment=NODE_ENV="production"
command=/usr/bin/nodejs server
process_name=<%= node['deploy'][@webapp_name][:environment][:dns_name] %>%(process_num)s
numprocs=1
directory=<%= node['deploy'][@webapp_name]['current_path'] %>
umask=022
autostart=true
autorestart=true
startsecs=2
startretries=3